{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Configuration file schema",
    "type": "object",
    "properties": {
        "key": {
            "description": "Binance API key",
            "type": "string"
        },
        "secret": {
            "description": "Binance API secret",
            "type": "string"
        },
        "databaseUrl": {
            "description": "Path to database",
            "type": "string"
        },
        "bucketSizeBtc": {
            "description": "Bucket size in BTC",
            "type": "number"
        },
        "profitMultiplier": {
            "description": "Price limit",
            "type": "number"
        },
        "forceSellInMinutes": {
            "description": "Max time to keep currency",
            "type": "number"
        },
        "cycleTimeMinutes": {
            "description": "Time of the cycle",
            "type": "number"
        },
        "symbols": {
            "description": "List of currencies",
            "type": "array"
        },
        "movingAverageWindow": {
            "description": "Moving average window",
            "type": "number"
        },
        "exchangeFee": {
            "description": "Exchange fee",
            "type": "number"
        }
    },
    "required": [
        "key", "secret"
    ]
}